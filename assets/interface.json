{
    "version": "v1.5.1",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource/base",
                "{PROJECT_DIR}/resource_bilibili"
            ]
        }
    ],
    "task": [
        {
            "name": "运行布魔",
            "entry": "tkfm_StartUp_StartUpByOption",
            "option": [
                "账号选择"
            ]
        },
        {
            "name": "自动领取好友体力",
            "entry": "tkfm_friend_领取好友体力"
        },
        {
            "name": "自动派遣",
            "entry": "tkfm_dispatch_自动派遣"
        },
        {
            "name": "自动炼金以及提交订单",
            "entry": "tkfm_alchemhy_自动炼金以及提交订单"
        },
        {
            "name": "自动购买商店物品",
            "entry": "tkfm_mall_自动购买商店物品"
        },
        {
            "name": "日常任务_自动升级两次和调教三次",
            "entry": "tkfm_daily1_完成日常任务_自动升级两次和调教三次"
        },
        {
            "name": "日常任务_自动消耗体力",
            "entry": "tkfm_daily2_自动领取日常任务奖励_消耗体力",
            "option": [
                "副本选择"
            ]
        },
        {
            "name": "日常任务_自动领取日常任务奖励",
            "entry": "tkfm_daily3_自动领取日常任务奖励_自动领取任务奖励"
        },
        {
            "name": "自动查看全境征才是否有领袖和菁英词条",
            "entry": "tkfm_recruitment_自动招募"
        },
        {
            "name": "自动领取协会奖励",
            "entry": "tkfm_association_自动领取协会奖励"
        }
    ],
    "option": {
        "账号选择": {
            "cases": [
                {
                    "name": "账号1",
                    "pipeline_override": {
                        "tkfm_StartUp_点击图标": {
                            "index": 0
                        }
                    }
                },
                {
                    "name": "账号2",
                    "pipeline_override": {
                        "tkfm_StartUp_点击图标": {
                            "index": 1
                        }
                    }
                },
                {
                    "name": "账号3",
                    "pipeline_override": {
                        "tkfm_StartUp_点击图标": {
                            "index": 2
                        }
                    }
                },
                {
                    "name": "账号4",
                    "pipeline_override": {
                        "tkfm_StartUp_点击图标": {
                            "index": 3
                        }
                    }
                }
            ]
        },
        "副本选择": {
            "cases": [
                {
                    "name": "限时活动45体力本——扫荡",
                    "pipeline_override": {
                        "tkfm_LimitedTime_activity_是否启用扫荡": {
                            "enabled": true
                        },
                        "tkfm_daily2_自动领取日常任务奖励_消耗体力_限时活动": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "限时活动45体力本——Auto",
                    "pipeline_override": {
                        "tkfm_LimitedTime_activity_是否启用扫荡": {
                            "enabled": false
                        },
                        "tkfm_daily2_自动领取日常任务奖励_消耗体力_限时活动": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "日常50银币本——扫荡",
                    "pipeline_override": {
                        "tkfm_daily2_自动领取日常任务奖励_消耗体力_银币副本": {
                            "enabled": true
                        },
                        "tkfm_daily2_每日政务_是否启用扫荡": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "日常50银币本——Auto",
                    "pipeline_override": {
                        "tkfm_daily2_自动领取日常任务奖励_消耗体力_银币副本": {
                            "enabled": true
                        },
                        "tkfm_daily2_每日政务_是否启用扫荡": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "日常50经验本——扫荡",
                    "pipeline_override": {
                        "tkfm_daily2_自动领取日常任务奖励_消耗体力_经验药水副本": {
                            "enabled": true
                        },
                        "tkfm_daily2_每日政务_是否启用扫荡": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "日常50经验本——Auto",
                    "pipeline_override": {
                        "tkfm_LimitedTime_activity_是否启用扫荡": {
                            "enabled": false
                        },
                        "tkfm_daily2_自动领取日常任务奖励_消耗体力_经验药水活动": {
                            "enabled": true
                        }
                    }
                },
                {
                    "name": "日常30情报本——扫荡",
                    "pipeline_override": {
                        "tkfm_daily2_自动领取日常任务奖励_消耗体力_情报副本": {
                            "enabled": true
                        },
                        "tkfm_daily2_每日政务_是否启用扫荡": {
                            "enabled": false
                        }
                    }
                },
                {
                    "name": "日常30情报本——Auto",
                    "pipeline_override": {
                        "tkfm_daily2_自动领取日常任务奖励_消耗体力_情报副本": {
                            "enabled": true
                        },
                        "tkfm_daily2_每日政务_是否启用扫荡": {
                            "enabled": false
                        }
                    }
                }
            ]
        }
    }
}